{
  "name": "triven-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:studio": "npx prisma studio",
    "db:gen": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:build": "bun run db:gen && bun run db:push",
    "db:reset": "bun run db:gen && bun run db:push --force-reset && bunx prisma db seed && bun run dev",
    "spec:init": "source $HOME/.local/bin/env && uvx --from git+https://github.com/github/spec-kit.git specify init",
    "spec:check": "source $HOME/.local/bin/env && uvx --from git+https://github.com/github/spec-kit.git specify check",
    "ollama:ps": "docker-compose ps ollama",
    "ollama:start": "docker-compose up ollama -d",
    "ollama:stop": "docker-compose stop ollama",
    "ollama:logs": "docker-compose logs -f ollama",
    "ollama:restart": "docker-compose restart ollama",
    "ollama:down": "docker-compose down ollama",
    "ollama:build": "docker-compose build ollama",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhook",
    "stripe:cleanup-duplicates": "bun run scripts/cleanup-duplicate-subscriptions.ts",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
